//运行DLL接口
import fsys
import process;

namespace process; 

//process.rundll("shell32.dll","Control_RunDLL")
class rundll{
	ctor(dll,arg,...){
		..process.execute("rundll32.exe",..fsys.shortpath(dll) +"," +arg,...); 
	} 
}
 
