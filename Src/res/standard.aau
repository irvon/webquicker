import fsys;

var aau_path = ...;

include = function (path) {
	if (string.startWith(path, "/")) {
		path = fsys.joinpath(io.fullpath("/pages/"), path);
	} elseif (string.sub(path, 2, 2) != ":") {
		// Ïà¶ÔÂ·¾¶
		path = fsys.joinpath(io.splitpath(aau_path).dir, path);
	}
	var func, err = loadcode(path);
	if (!func) {
		response.write(err);
	} else {
		call(func, path);
	}
}