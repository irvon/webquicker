"encode":{
	"type":"function",
	"declarations":"string(table tab)",
	"assertFunc":"assertJsonEquals",
	"casenums":11,
	"case1":{
		"desc":"AAuto table表（Hash数组），v为字符串",
		"input":[{"t1":"hello world","t2":"中文","t3":"\\\/\b\f\n\r\t\""}],
