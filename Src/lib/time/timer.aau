
namespace time.timer{
	var tk = 0;
	
	start = function(){
		tk = ..time.tick()
	}
	
	tick = function(){
		return ..time.tick() - tk; 
	}
	
	test = function( loop,f ){
		var t = 0;
		for(i=1;loop;1){
			start()
			f();
			t += tick();
		}
		return t \ loop
	}
	
}

